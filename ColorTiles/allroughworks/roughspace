x=x-1;
y=y-1
if((x<3)&&(y<3)&&(x>-1)%%(y>-1))



if((x==1)&&(y==1)){a=0;b=0}else if((x==1)&&(y==2)){a=0;b=1}else if((x==1)&&(y==3)){a=0;b=3}else if((x==2)&&(y==1)){a=1;b=0}else if((x==2)&&(y==2)){a=1;b=1}else if((x==2)&&(y==3)){a=1;b=2}else if((x==3)&&(y==1)){a=2;b=0}else if((x==3)&&(y==2)){a=2;b=1}else if((x==3)&&(y==3)){a=2;b=2;}